						<xs:documentation>Size of LUT</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="LookupTableType">
		<xs:simpleContent>
			<xs:extension base="LUTListType">
